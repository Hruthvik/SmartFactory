POST http://localhost:4000
Content-Type: application/json
X-REQUEST-TYPE: GraphQL

mutation itemData($id : Number!, $device : String!,$name : String!, $humidity: String!){
  addHumidity(id : $id, device : $device,name : $name, humidity:$humidity) 
} 

{
    "id" : 3,
    "device" : "temperature",
    "name" : "temp",
    "humidity" : "27"
}



